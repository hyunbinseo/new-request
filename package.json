{
	"name": "new-request",
	"description": "Type-check and autocomplete for 3rd party REST API requests",
	"version": "0.0.11",
	"engines": {
		"node": ">=18"
	},
	"type": "module",
	"scripts": {
		"build": "tsup --format cjs,esm --dts --clean && publint",
		"prepublishOnly": "npm run build",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@types/node": "20",
		"prettier": "^3.1.0",
		"publint": "^0.2.5",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2"
	},
	"tsup": {
		"entry": [
			"src/index.ts",
			"src/email/send-grid/v3/index.ts",
			"src/sms/nhn/3.0/index.ts",
			"src/sms/twilio/2010-04-01/index.ts",
			"src/tts/naver/v1/index.ts",
			"src/misc/neis/info/index.ts"
		]
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./email/send-grid/v3": {
			"import": {
				"types": "./dist/email/send-grid/v3/index.d.ts",
				"default": "./dist/email/send-grid/v3/index.js"
			},
			"require": {
				"types": "./dist/email/send-grid/v3/index.d.cts",
				"default": "./dist/email/send-grid/v3/index.cjs"
			}
		},
		"./sms/nhn/3.0": {
			"import": {
				"types": "./dist/sms/nhn/3.0/index.d.ts",
				"default": "./dist/sms/nhn/3.0/index.js"
			},
			"require": {
				"types": "./dist/sms/nhn/3.0/index.d.cts",
				"default": "./dist/sms/nhn/3.0/index.cjs"
			}
		},
		"./sms/twilio/2010-04-01": {
			"import": {
				"types": "./dist/sms/twilio/2010-04-01/index.d.ts",
				"default": "./dist/sms/twilio/2010-04-01/index.js"
			},
			"require": {
				"types": "./dist/sms/twilio/2010-04-01/index.d.cts",
				"default": "./dist/sms/twilio/2010-04-01/index.cjs"
			}
		},
		"./tts/naver/v1": {
			"import": {
				"types": "./dist/tts/naver/v1/index.d.ts",
				"default": "./dist/tts/naver/v1/index.js"
			},
			"require": {
				"types": "./dist/tts/naver/v1/index.d.cts",
				"default": "./dist/tts/naver/v1/index.cjs"
			}
		},
		"./misc/neis/info": {
			"import": {
				"types": "./dist/misc/neis/info/index.d.ts",
				"default": "./dist/misc/neis/info/index.js"
			},
			"require": {
				"types": "./dist/misc/neis/info/index.d.cts",
				"default": "./dist/misc/neis/info/index.cjs"
			}
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"author": "Hyunbin Seo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyunbinseo/new-request.git"
	},
	"bugs": {
		"url": "https://github.com/hyunbinseo/new-request/issues"
	},
	"homepage": "https://github.com/hyunbinseo/new-request#readme"
}
